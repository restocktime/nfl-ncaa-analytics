{
  "name": "football-analytics-system",
  "version": "1.0.0",
  "description": "Real-time football analytics system with predictive modeling",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:server": "ts-node-dev --respawn --transpile-only src/dev-server.ts",
    "start": "node dist/index.js",
    "start:demo": "npm run build && node dist/dev-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration,e2e",
    "test:integration": "jest --testPathPattern=integration",
    "test:integration:staging": "STAGING_API_URL=$STAGING_API_URL STAGING_WS_URL=$STAGING_WS_URL node tests/deployment/integration-tests.js",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:critical": "jest --testPathPattern=critical",
    "test:load": "jest --testPathPattern=load-testing",
    "test:smoke:production": "node tests/deployment/smoke-tests.js",
    "test:websocket:production": "node tests/deployment/websocket-test.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "security:validate": "npm audit --audit-level=high"
  },
  "keywords": [
    "football",
    "analytics",
    "predictions",
    "microservices"
  ],
  "author": "Football Analytics Team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/redis": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "redis-memory-server": "^0.10.0",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "inversify": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.12.1",
    "ml-regression": "^6.3.0",
    "ml-xgboost": "^1.1.2",
    "morgan": "^1.10.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.2",
    "shap": "^1.1.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0",
    "webshap": "^0.1.4",
    "winston": "^3.10.0",
    "ws": "^8.18.3",
    "yamljs": "^0.3.0"
  }
}
